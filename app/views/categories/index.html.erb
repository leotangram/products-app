<h1>Listado Categorías</h1>
 
<table>
  <tr>
    <th>Nombre</th>
  </tr>
 
  <% @categories.each do |category| %>
    <tr>
      <td><%= category.name %></td>
      <td><%= link_to 'Ver', category_path(category) %></td>
      <td><%= link_to 'Editar', edit_category_path(category) %></td>
    </tr>
  <% end %>
</table>

<%= link_to 'Nueva Categoría', new_category_path %>
<%= link_to 'Inicio', controller: 'wellcome' %>